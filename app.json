{
    "name": "ToolJet",
    "description": "ToolJet is an open-source low-code framework to build and deploy internal tools.",
    "website": "",
    "repository": "https://github.com/AlexanderSpringer/web3-nest-postgres-auth",
    "logo": "",
    "success_url": "/",
    "scripts":{
      "predeploy": "npm install && npm run build"
    },
    "env": {
      "NODE_ENV": {
        "description": "Environment [production/development]",
        "value": "production"
      }
    },
    "formation": {
      "web": {
        "quantity": 1
      }
    },
    "image": "heroku/nodejs",
    "addons": ["heroku-postgresql"],
    "buildpacks": [
      {
        "url": "heroku/nodejs"
      }
    ]
  }